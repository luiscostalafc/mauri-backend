{
  "name": "backend",
  "version": "0.12.4",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node ace serve -w -i='.docker'",
    "dev:server": "node ace serve -w -i='.docker'",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "migrate": "node ace migration:run",
    "seed": "node ace db:seed --files=database/seeders/_DatabaseSeeder",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^3.0.0",
    "@types/faker": "^5.1.7",
    "@types/jsdom": "^16.2.7",
    "@types/supertest": "^2.0.10",
    "adonis-preset-ts": "^2.0.0",
    "cross-blob": "^2.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-adonis": "^1.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "^5.0.0",
    "faker": "^5.4.0",
    "get-port": "^5.1.1",
    "git-commit-msg-linter": "^3.0.4",
    "japa": "^3.1.1",
    "jsdom": "^16.5.1",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.1",
    "supertest": "^6.1.3",
    "typescript": "~4.1",
    "youch": "^2.1.1",
    "youch-terminal": "^1.0.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^5.1.1",
    "@adonisjs/core": "^5.0.4-preview-rc",
    "@adonisjs/lucid": "^10.0.0",
    "@adonisjs/mail": "^5.2.3",
    "@adonisjs/repl": "^1.0.0",
    "aws-sdk": "^2.866.0",
    "luxon": "^1.26.0",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "phc-argon2": "^1.1.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
