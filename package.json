{
  "name": "backend",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "node ace serve --watch",
    "start:prod": "node ./build/server.js",
    "build": "node ace build --production --ignore-ts-errors",
    "dev:server": "node ace serve",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "migrate": "node ace migration:run",
    "seed": "node ace db:seed --files=database/seeders/_DatabaseSeeder",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jsdom": "^16.2.13",
    "@types/supertest": "^2.0.11",
    "adonis-preset-ts": "^2.1.0",
    "cross-blob": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^3.4.1",
    "execa": "^5.0.0",
    "faker": "^5.5.3",
    "get-port": "^5.1.1",
    "git-commit-msg-linter": "^3.2.8",
    "japa": "^3.1.1",
    "jsdom": "^16.7.0",
    "pino-pretty": "^4.8.0",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "typescript": "^4.1.6",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/assembler": "^3.1.1",
    "@adonisjs/auth": "^5.1.1",
    "@adonisjs/core": "~5.0.4-preview-rc-2.1",
    "@adonisjs/lucid": "^10.0.0",
    "@adonisjs/mail": "^7.2.4",
    "@adonisjs/repl": "^1.1.6",
    "aws-sdk": "^2.1029.0",
    "g": "^2.0.1",
    "luxon": "^1.28.0",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "phc-argon2": "^1.1.2",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.20",
    "yarn": "^1.22.17"
  },
  "engines": {
    "node": "14.x"
  }
}
