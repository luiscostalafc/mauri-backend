{
  "name": "backend",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "build": "node ace build",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "migrate": "node ace migration:run",
    "seed": "node ace db:seed --files=_DatabaseSeeder",
    "dev:server": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^2.1.3",
    "@types/jest": "^26.0.3",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.0.14",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "adonis-preset-ts": "^1.0.4",
    "eslint": "^7.3.1",
    "eslint-plugin-adonis": "^1.0.14",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "pino-pretty": "^4.0.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0",
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.5",
    "@types/hapi__joi": "^17.1.6",
    "@types/ioredis": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^6.9.3",
    "@adonisjs/auth": "^4.2.4",
    "@adonisjs/core": "^5.0.0-preview-rc-1.9",
    "@adonisjs/fold": "^6.3.5",
